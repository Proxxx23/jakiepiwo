{
    "name": "proxxx23/degustatorapi",
    "description": "API aplikacji Degustator",
    "keywords": ["framework", "laravel", "degustator", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-intl": "^7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "barryvdh/laravel-cors": "~2.0.0",
        "fideloper/proxy": "~4.4.0",
        "guzzlehttp/guzzle": "~6.5.0",
        "laravel/framework": "~6.20.0",
        "laravel/helpers": "~1.4.0",
        "laravel/tinker": "~2.5.0",
        "nesbot/carbon": "~2.41.0",
        "symfony/cache": "~5.1.0",
        "symfony/dotenv": "~5.1.0"
    },
    "require-dev": {
        "drewm/mailchimp-api": "~2.5.0",
        "filp/whoops": "~2.7.0",
        "mockery/mockery": "~1.4.0",
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "~9.4.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/migrations"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
