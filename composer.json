{
    "name": "proxxx23/degustatorapi",
    "description": "API aplikacji Degustator",
    "keywords": ["framework", "laravel", "degustator", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "barryvdh/laravel-cors": "0.11.4",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.4",
        "laravel/helpers": "^1.0",
        "laravel/tinker": "~1.0",
        "nesbot/carbon": "^2.5",
        "symfony/dotenv": "5.0.1",
        "psr/cache": "1.0.1"
    },
    "require-dev": {
        "drewm/mailchimp-api": "^2.5",
        "filp/whoops": "~2.0",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "~8.0",
        "roave/security-advisories": "dev-master",
        "rector/rector": "^0.6"
    },
    "autoload": {
        "classmap": [
            "database/migrations"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
